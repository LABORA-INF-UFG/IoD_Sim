{
    "restructuredtext.confPath": "${workspaceFolder}\\docs",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
    },
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "editor.tabSize": 2,
    "workbench.tree.indent": 5,
    "files.associations": {
        "array": "cpp",
        "hash_map": "cpp",
        "string_view": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "unordered_map": "cpp",
        "*.tcc": "cpp",
        "random": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "utility": "cpp"
    }
}
